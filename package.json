        "currypath"           : ">= 3.0.0, < 4.0.0",
        "containers"          : ">= 3.0.0, < 4.0.0",
        "directory"           : ">= 3.0.0, < 4.0.0",
        "filepath"            : ">= 3.0.0, < 4.0.0",
        "flatcurry"           : ">= 5.0.0, < 6.0.0",
        "flatcurry-annotated" : ">= 4.0.0, < 5.0.0",
        "io-extra"            : ">= 3.0.0, < 4.0.0",
        "scc"                 : ">= 3.0.0, < 4.0.0",
