        "propertyfile"        : ">= 3.0.0, < 4.0.0",
        "queue"               : ">= 3.0.0, < 4.0.0",
        "redblacktree"        : ">= 3.0.0, < 4.0.0",
        "time"                : ">= 3.0.0, < 4.0.0",
        "transformers"        : ">= 3.0.0, < 4.0.0",
        "wl-pprint"           : ">= 3.0.0, < 4.0.0"
